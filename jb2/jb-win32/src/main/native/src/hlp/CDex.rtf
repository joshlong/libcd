\ql \li0\ri0\nowidctlpar\faauto\rin0\lin0\itap0 \f1\fs20\lang1031\langfe1031\cgrid\langnp1031\langfenp1031 {\cs25\super\insrsid9128616\charrsid9128616 K}{\f0\fs24\insrsid9128616  CD-Quality}}K{\footnote \pard\plain 

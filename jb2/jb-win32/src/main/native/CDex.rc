        MENUITEM "Generate &CUE sheet",         ID_TOOLS_GENERATECUESHEET
        MENUITEM "&Detect pre-gaps",            ID_TOOLS_DETECTPRE
        MENUITEM "Read UPC/ISRC code",          ID_TOOLS_READUPC
